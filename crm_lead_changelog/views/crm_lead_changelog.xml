<?xml version="1.0" encoding="UTF-8"?>

<openerp>
    <data noupdate="0">

        <record id="view_crm_lead_changelog_assets" model="ir.ui.view">
            <field name="name">crm_lead_changelog assets</field>
            <field name="model"></field>
            <field name="type">qweb</field>
            <field name="mode">extension</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="inherit_id" ref="web.assets_backend" />
            <field name="arch" type="xml">
                <xpath expr="." position="inside">
                    <link rel="stylesheet" href="/crm_lead_changelog/static/src/css/crm_lead_changelog.css" />
                </xpath>
            </field>
        </record>

        <record id="menu_leads_opportunities_changelog" model="ir.ui.menu" >
            <field name="name">Leads/Opportunities changelog</field>
            <field name="sequence" eval="10" />
            <field name="parent_id" ref="base.menu_reporting" />
        </record>

    </data>
</openerp>
