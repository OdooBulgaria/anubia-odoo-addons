<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    	
    	<record id="view_picking_out_form_uos_extended" model="ir.ui.view">
            <field name="name">stock.picking.out.form.uos.extended</field>
            <field name="model">stock.picking</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="stock.view_picking_out_form"/>
            <field name="arch" type="xml">
           		<data>
					<xpath expr="//form/notebook/page//field[@name='move_lines']//tree//field[@name='product_uos']" position="before">
	            		<field name="product_uos_qty" 
            		       	   groups="product.group_uos" 
            		           on_change="onchange_uos_quantity(product_id, product_uos_qty, product_uos, product_uom)"/>
               		</xpath>
           		</data>
<!--                 <form string="Delivery Orders"> -->
                    
            </field>
        </record>
    	
    </data>
</openerp>