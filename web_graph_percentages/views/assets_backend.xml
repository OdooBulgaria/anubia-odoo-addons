<?xml version="1.0" encoding="UTF-8"?>

<openerp>
    <data noupdate="0">

        <record id="view_web_graph_percetages_assets" model="ir.ui.view">
            <field name="name">Web Graph Percetages assets</field>
            <field name="model"></field>
            <field name="type">qweb</field>
            <field name="mode">extension</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="inherit_id" ref="web_graph.assets_backend" />
            <field name="arch" type="xml">
                <xpath expr="//script[@src='/web_graph/static/src/js/graph_view.js']" position="replace">
                    <script type="text/javascript" src="/web_graph_percentages/static/src/js/graph_view.js" />
                </xpath>

                <xpath expr="//script[@src='/web_graph/static/src/js/pivot_table.js']" position="replace">
                    <script type="text/javascript" src="/web_graph_percentages/static/src/js/pivot_table.js" />
                </xpath>

                <xpath expr="//script[@src='/web_graph/static/src/js/graph_widget.js']" position="replace">
                    <script type="text/javascript" src="/web_graph_percentages/static/src/js/graph_widget.js" />
                </xpath>
            </field>
        </record>

    </data>
</openerp>
